{

    "builds": [{
        "src": "visualisation_dashboard/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"
        }
    }],
    "routes":[
        {
        "src": "/(.*)",
        "dest": "visualisation_dashboard/wsgi.py"
        }
    ]
}
