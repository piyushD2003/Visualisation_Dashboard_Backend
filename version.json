{
    "version": 3,
    "builds": [
      {
        "src": "visualisation_dashboard/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.11"
        }
      }
    ],
    "installCommand": "pip install -r requirements.txt",
    "buildCommand": "python manage.py migrate && python manage.py collectstatic --noinput",
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "visualisation_dashboard/wsgi.py"
      }
    ]
  }