{

    "builds": [{
        "src": "visualisation_dashboard/wsgi.py",
        "use": "@vercel/python",
        "config": {
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9",
        "buildCommand": "bash setup.sh"
        }
    }],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
        "src": "/(.*)",
        "dest": "visualisation_dashboard/wsgi.py"
        }
    ]
}
